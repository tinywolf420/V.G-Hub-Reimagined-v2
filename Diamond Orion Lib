	function AutoHeal()
		while _G.AutoHeal == true do
			wait(.5)
			ReplicatedStorage.API["MonitorAPI/HealWithDoctor"]:FireServer()
		end
	end

	function NoBabyPickUp()
		while _G.NoBabyPickUp == true do
			ReplicatedStorage.API["FamilyAPI/CreateFamily"]:InvokeServer()
